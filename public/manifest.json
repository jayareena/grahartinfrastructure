{
  "manifest_version": 2,
  "name": "Grahart Infrastructure Extension",
  "version": "1.0",
  "permissions": [
    "tabs",
    "contextMenus",
    "downloads"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "icons": {
    "16": "favicon.ico",
    "48": "logo.png",
    "128": "logo512.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  }
}
